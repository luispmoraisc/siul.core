{
  "targets": {
    "pre-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx style-dictionary build --config packages/ds-tokens/config.json"
      },
      "parallel": false
    },
    "post-dictionary": {
      "executor": "nx:run-script",
      "options": {
        "script": "node scripts/post-dictionary.js"
      },
      "dependsOn": ["pre-build"]
    },
    "build": {
      "dependsOn": ["post-dictionary"]
    }
  }
}
